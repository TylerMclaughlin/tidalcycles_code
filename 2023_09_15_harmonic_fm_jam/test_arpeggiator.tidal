d1 $ (|+ n "[0 7]/4") $ n "[0,3, 7, 10, 14]" # m 1 

d1 $ (|+ n "[0 7]/4") $ n "[0,3, 7, 12, 15, 19]" # m 1 

d3 $ (|- n "[0 12]/4") $ (|+ n "[0 7]/4") $ n "[ 10, 14, 17, 22, 26, 34]" # m 2 

d2 $ cc1 11 "10 40 20 30" 

d2 $  cc1 11 (segfunc 16 $
     (fast "[[4 3 2 5 4]@7 4]" $ (range 0 12 sine |+ 26) |+ "[0!7 16]/8" ))

d2 $  cc1 11 (segfunc 16 $
     (fast "4 2 5 4" $ (range 0 12 sine |+ 26) |+ "[0!7 128]/8" ))

d4 $  cc1 12 (segfunc 16 $
     (fast "4 2 5 4" $ (range 0 12 sine |+ 26) |+ "[0!7 128]/8" ))

d3 $ cc1 12 "10 40 20 30" 

d2 $ cc1 11 (segfunc 16 $ (fast ("4 3 2 5 4" |* 3) $ range 0 12 sine |+ 26 ))

d2 $ cc1 11 (fast "4 3 2 4 5" $ range 1 128 $ sine )
