:script "~/Music/ableton_songs/toolz.tidal"

setbpm 70 

d4 $ drumz $ fast 2 $ n "0 [[0,2] 0] 0 [0,2]" # m 1

d1 $ n "7@2 5 4  2 ~ ~ <4 -3>  0 ~ ~ 4  2 ~ ~ ~ " # m 2

d2 $ struct  "[[~ ] [~ t t ~]]*2" $ n "<[0,4,7] [2,5,7]>*2" # m 3

d3 $ struct  "[<~ [~ t t ~]>  [~ t t ~]]*2" $
     n "<[0,4,7] [2,5,7]>" # m 3

d5 $ n "[~ ] [<4 0>@3 <5 2>@5]" # m 4

