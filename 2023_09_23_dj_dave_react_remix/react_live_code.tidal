let pigments_intro = once $ qtrigger $ drumz $ n 0 # m 1 
    pigments_verse = once $ qtrigger $ drumz $ n 1 # m 1 
    pigments_break = once $ qtrigger $ drumz $ n 2 # m 1 
    pigments_chorus = once $ qtrigger $ drumz $ n 3 # m 1 
    pigments_outro = once $ qtrigger $ drumz $ n 4 # m 1 
    bass_intro = once $ qtrigger $ drumz $ n 5 # m 1 
    bass_verse = once $ qtrigger $ drumz $ n 6 # m 1 
    bass_chorus = once $ qtrigger $ drumz $ n 7 # m 1 
    keys_verse = once $ qtrigger $ drumz $ n 8 # m 1 
    keys_break = once $ qtrigger $ drumz $ n 9 # m 1 
    arp_c2 = once $ qtrigger $ drumz $ n 10 # m 1 
    arp_outro = once $ qtrigger $ drumz $ n 11 # m 1 
    vox_verse = once $ qtrigger $ drumz $ n 12 # m 1 
    vox_break = once $ qtrigger $ drumz $ n 13 # m 1 
    vox_chorus = once $ qtrigger $ drumz $ n 14 # m 1 
    vox_outro = once $ qtrigger $ drumz $ n 15 # m 1 
    riser_break = once $ qtrigger $ drumz $ n 16 # m 1 
    riser_chorus = once $ qtrigger $ drumz $ n 17 # m 1 
    riser_outro = once $ qtrigger $ drumz $ n 18 # m 1 
    vfx_intro = once $ qtrigger $ drumz $ n 19 # m 1 
    vfx_verse = once $ qtrigger $ drumz $ n 20 # m 1 
    vfx_break = once $ qtrigger $ drumz $ n 21 # m 1 
    shaker = once $ qtrigger $ drumz $ n 22 # m 1 
    synth_c2 = once $ qtrigger $ drumz $ n 23 # m 1 
    synth_outro = once $ qtrigger $ drumz $ n 24 # m 1 

resetCycles

getnow

-- intro
do
  hush
  bass_intro
  pigments_intro
  vfx_intro


-- verse
do
  hush
  drums_verse
  bass_verse
  pigments_verse
  keys_verse
  shaker
  vox_verse
  vfx_verse

-- break 
do
  hush
  riser_break
  vox_break
  vfx_break
  pigments_break
  keys_break

-- chorus
do
  hush
  drums_chorus
  riser_chorus
  pigments_chorus
  bass_chorus
  vox_chorus 

-- chorus 2
do
  arp_c2
  synth_c2

-- outro
do
  hush
  riser_outro
  pigments_outro
  arp_outro
  vox_outro
  synth_outro
