
-- These are synced in pigments

-- 19
d1 $ (|- n "[0 2 0 4]/4") $ n (tScale' 19 "0 3 6 8 11 14 17" ("[0 2 [4 5] 6]*2" |- "4 3 5 2" |- "[00 2 1 3]/8" )) # m 1

-- 31
-- set to 442.3
d2 $ (|+ n "[0 3 0 6]/4") $ (|- n 6) $  n (tScale' 31 "0 5 10 13 18 23 28" ("[0 2 4 6 ]*2" |- "4 3 5 2" |- "[0 2 1 3]/8")) # m 2

d2 $ n ( tScale' 31 "0 3 6 9 12 15 18 21 24 27" ("0 .. 31")) # m 2

d2 $ sus 3 ("4 3 4 2 3 1 2" |/ 8) $ sliceDiv 0 8 21 10 $ struct "t(9, 21)" $  n "[0, 8, 18, 26, 36]"  # m 2

d2 $ sus 3 ("4 3 4 2 3 1 2" |/ 8) $ sliceDiv 0 8 21 10 $ struct "t(9, 21)" $  n "[0, 8, 18, 26, 36]"  # m 2

d2 $ slow 1.5 $ (|- n 6) $ 
    (|- n "[0 5 <10 -5> < -10 13 >]/4") $
    sus 3 ("4 3 4 2 3 1 2" |/| 8) $
    gas 3 ("4 3 4 2 3 1 2" |/| 4) $
    sliceDiv 0 7 21 14 $
    struct "t(<9 7 13 5 11 3 14>, 21)" $ n "[18, 26, 36]" # m 2
 -- n "[0, 8, 18, 26, 36]"  # m 2

d1 $ slow 1.5 $ (|- n "[0 3 < 6 -3> < -6 8 >]/4") $
    sus 3 ("4 3 4 2 3 1 2" |/| 8) $
    gas 3 ("4 3 4 2 3 1 2" |/| 4) $
    sliceDiv 0 7 21 14 $
    struct (inv "t(<9 7 13 5 11 3 14>, 21)") $  n "[0, 3, 5]"  # m 1
